<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_runbot_config_settings_view" model="ir.ui.view">
            <field name="model">runbot.config.settings</field>
            <field name="inherit_id" ref="runbot.view_runbot_configuration" />
            <field name="arch" type="xml">
                <group position="after">
                    <group name="runbot_letsencrypt">
                        <label for="id" string="Letsencrypt"/>
                        <div>
                            <div>
                                <field name="default_max_certs_per_week" class="oe_inline"/>
                                <label for="default_max_certs_per_week"/>
                            </div>
                        </div>
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>
